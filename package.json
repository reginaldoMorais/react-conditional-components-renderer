{
  "name": "react-conditional-components-renderer",
  "version": "1.0.6",
  "description": "React library with conditional components to rendering",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:w": "jest --watchAll",
    "test:v": "jest --verbose",
    "test:c": "jest --coverage",
    "test:d": "jest --debug",
    "test": "jest",
    "clean": "rm -Rf dist",
    "build": "npm run clean && tsc --skipLibCheck -p .",
    "husky-install": "husky install && chmod +x -Rf .husky"
  },
  "keywords": [
    "react",
    "typescript",
    "conditional",
    "components"
  ],
  "author": "reginaldoMorais <reginaldo.cmorais@gmail.com>",
  "repository": "https://github.com/reginaldoMorais/react-conditional-components-renderer.git",
  "license": "ISC",
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^5.0.1"
  },
  "peerDependencies": {
    "react": "17.x",
    "react-dom": "17.x",
    "react-router-dom": "5.x"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  }
}
